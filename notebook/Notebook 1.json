					"from pyspark.sql import SparkSession\n",
					"from pyspark.sql.types import * \n",
					"\n",
					"account_name = \"jessicaadls\"\n",
					"container_name = \"jessica\"\n",
					"relative_path = \"titanic\"\n",
					"adls_path = 'abfss://%s@%s.dfs.core.windows.net/%s' % (container_name, account_name, relative_path)\n",
					"\n",
					"\n",
					"\n",
					"data = spark.read.option('header', 'true').option('delimiter', ',').csv(adls_path + '/titanic.csv')\n",
					"\n",
					"data = data.toPandas()\n",
					"\n",
					"data.head()"
				],
				"execution_count": 3
			},
			{
				"cell_type": "code",
				"source": [
					"# filter by sex and select only females\n",
					"data_females = data[data['Sex'] == 'female']\n",
					"#sort from highest fare\n",
					"data_females.sort('Fare')\n",
					"# save data to same datalake, use different filename\n",
					"\n",
					"data_females.head()"
